{
  "name": "scription",
  "version": "1.1.0",
  "author": "Cameron Baumgartner https://github.com/cameronbaumgartner <cameron.h.baumgartner@gmail.com>",
  "engines": {
    "node": "^12.x",
    "npm": "6.x"
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "prod": "NODE_ENV=production webpack & NODE_ENV=production node -r dotenv/config server/server.js",
    "prod-windows": "concurrently \"cross-env NODE_ENV=production webpack\" \"node -r dotenv/config server/server.js\"",
    "dev": "NODE_ENV=development webpack serve --open --hot & NODE_ENV=development nodemon -r dotenv/config server/server.js",
    "dev-windows": "concurrently \"cross-env NODE_ENV='development' webpack serve --open --hot\" \"nodemon -r dotenv/config server/server.js\"",
    "test": "NODE_ENV=test jest --verbose --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "build"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@webpack-cli/serve": "^1.2.1",
    "babel-loader": "^8.2.2",
    "bcrypt": "^5.0.0",
    "concurrently": "^4.1.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^5.2.1",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "exports-loader": "^2.0.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.3",
    "pg": "^8.5.1",
    "postcss": "^8.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "script-loader": "^0.7.2",
    "shx": "^0.3.3",
    "source-map-loader": "^2.0.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.3.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.4",
    "serve": "^11.3.2",
    "webpack-dev-server": "^3.11.1",
    "webpack-livereload-plugin": "^2.3.0"
  }
}
